				            <th align="center" style="text-align: center; vertical-align: middle;">Address</th>
				            
				        </tr>
				  </thead>
				<tbody id="tbodyDet">
			       <% for(var i=0; i < data.length; i++) {%> 	
                   <td align="center" style="text-align: center; vertical-align: middle;"><%=data[i].FlightNo%></td>
		           <td align="center" style="text-align: center; vertical-align: middle;"><%=data[i].PNR%></td>
                   <td align="center" style="text-align: center; vertical-align: middle;"><%=data[i].Name%></td>
				   <td align="center" style="text-align: center; vertical-align: middle;"><%=data[i].quantity%></td>
				   <td align="center" style="text-align: center; vertical-align: middle;"><%=data[i].address%></td></tr> 
				   <%}%>
                </tbody>
				</table>
