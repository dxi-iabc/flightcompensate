       	<table class="table table-bordered" style="border-collapse:collapse;">
				    <thead>
				        <tr>
				            
				            <th align="center" style="text-align: center; vertical-align: middle;">Flight No</th>
				            <th align="center" style="text-align: center; vertical-align: middle;">Passenger PNR </th>
                            <th align="center" style="text-align: center; vertical-align: middle;">Name</th>
